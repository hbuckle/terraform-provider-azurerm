{
  "Deadline": "10m",
  "Enable": [
    "errcheck",
    "unconvert",
    "gofmt",
    "goimports",
    "gotype",
    "ineffassign",
    "interfacer",
    "misspell",
    "structcheck",
    "unparam",
    "unused",
    "varcheck",
    "vet"
  ],
  "EnableGC": true,
  "Linters": {
    "errcheck": {
      "Command": "errcheck -abspath {not_tests=-ignoretests} -ignore github.com/hashicorp/terraform/helper/schema:ForceNew|Set -ignore io:Close"
    }
  },
  "Sort": [
    "path",
    "line"
  ],
  "Vendor": true,
  "WarnUnmatchedDirective": true
}
